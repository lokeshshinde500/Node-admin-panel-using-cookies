<%- include("header")%>

    <div class="content">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">Admin data</strong>
                        </div>
                        <div class="card-body">
                            <table class="table">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">No.</th>
                                        <th scope="col">Full Name</th>
                                        <th scope="col">Email</th>
                                        <th scope="col">Comment</th>
                                        <th scope="col">Gender</th>
                                        <th scope="col">Hobby</th>
                                        <th scope="col">Profile</th>
                                        <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%let no=1; %>
                                        <%adminData.forEach((v,i)=>{ %>
                                            <tr>
                                                <td>
                                                    <%=no++%>
                                                </td>
                                                <td>
                                                    <%=v.name%>
                                                </td>
                                                <td>
                                                    <%=v.email%>
                                                </td>
                                                <td>
                                                    <%=v.comment%>
                                                </td>
                                                <td>
                                                    <%=v.gender%>
                                                </td>

                                                <td>
                                                    <%=v.hobby%>
                                                </td>
                                                <td>
                                                    <img src="<%=v.image%>" height="50" alt="" />
                                                </td>
                                                <td>
                                                    <a href="/admin/deleteRecord/<%=v.id%>"
                                                        class="mr-2 text-danger ">delete</a>
                                                    <a href="/admin/updateRecord/?id=<%=v.id%>"
                                                        class="mr-2 text-success">update</a>
                                                </td>
                                            </tr>
                                            <% })%>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div><!-- .animated -->
    </div><!-- .content -->

    <div class="clearfix"></div>

    <%- include("footer")%>